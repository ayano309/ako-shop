      %label.sidebar-category-label
        = link_to "受注一覧", dashboard_orders_path, class:"sidebar-category-label_link"
    %h2.side-admin-main 商品管理
    .d-flex.flex-column
      %label.sidebar-category-label
        = link_to "商品一覧", dashboard_products_path, class:"sidebar-category-label_link"
