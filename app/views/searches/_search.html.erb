<%= form_with url: search_path, method: :get, local: true do |f| %>
  <div class="form-inline product_form" id="search-wrap">
    <div class="form-group">
      <%= f.text_field :content , id: "search-text"%>
      <%= f.hidden_field :method, :value => "patical" %>
    </div>
    <div class="input-group">
      <button type="submit" class="btn header-search-button"><i class="fas fa-search header-search-icon"></i></button>
    </div>
  </div>
<% end %>